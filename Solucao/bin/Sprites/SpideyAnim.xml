<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="spidey">
    <sequence name="run"  start="0" end="3" loop="true" />
    <sequence name="jump"  start="0" end="1" loop="true" />
</animation>

